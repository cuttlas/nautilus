# Anthropic API key (from https://console.anthropic.com)
ANTHROPIC_API_KEY=sk-ant-...

# Google AI API key (from https://aistudio.google.com/app/apikey)
GOOGLE_GENERATIVE_AI_API_KEY=

# Tavily API key (from https://tavily.com)
TAVILY_API_KEY=tvly-...

# Telegram Bot Token (from @BotFather)
TELEGRAM_BOT_TOKEN=

# Your Telegram Chat ID
TELEGRAM_CHAT_ID=

# GitHub Personal Access Token (fine-grained, repo scope)
GITHUB_TOKEN=

# The project's data repo (owner/repo format)
DATA_REPO_URL=user/sea-nomad

# The project's Cloudflare Pages URL
SITE_URL=https://sea-nomad.pages.dev

# Optional: override the default model (defaults to gemini-2.5-flash)
DEFAULT_MODEL=gemini-2.5-flash

# Optional: blindspot detection interval in minutes (default: 120)
# BLINDSPOT_INTERVAL_MINUTES=120
